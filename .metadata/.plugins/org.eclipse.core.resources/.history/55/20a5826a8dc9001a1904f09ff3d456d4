package com.he.day04;

public class Zuoyongyu {
    //成员变量（全局变量）
    String name = "张三";
    int age = 24;
    public static void main(String[] args) {
        int a=1;  // 局部变量定义在函数整体内，函数参数列表里，代码块{}中
        int b=2;
        System.out.println(a);
        System.out.println("姓名是："+ "name" +"，年龄是"+ "age");
    }

}
