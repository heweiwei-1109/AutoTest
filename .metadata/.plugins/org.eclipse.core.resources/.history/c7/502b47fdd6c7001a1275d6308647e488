package com.he.day03;

/** 根据月份获取季节
 * @author 土豆01
 *
 */
public class SwitchDemo {
    public static void main(String[] args) {
        int mounth =5;
        switch (mounth) {
        case 1:
            System.out.println("春季");
            break;
        case 2:
            System.out.println("春季");
            break;
        case 3:
            System.out.println("春季");
            break;
        case 4:
            System.out.println("夏季");
            break;
        case 5:
            System.out.println("夏季");
            break;
        case 6:
            System.out.println("夏季");
            break;
        case 7:
            System.out.println("秋季");
            break;
        case 8:
            System.out.println("秋季");
            break;
        case 9:
            System.out.println("秋季");
            break;
        case 10:
            System.out.println("冬季");
            break;
        case 11:
            System.out.println("冬季");
            break;
        case 12:
            System.out.println("冬季");
            break;
        default:
            System.out.println("月份输入不正确");
            break;
        }
    }

}
